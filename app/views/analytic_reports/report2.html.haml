        -if defined?(@election) && @election.id.to_s == params[:id]
          .span2#likeh4
            = link_to "Save as PDF", '/analytic_report.pdf?rn=2;id='+params[:id]
        .span1#statebacknext
          = link_to 'Back', :back
      .row
