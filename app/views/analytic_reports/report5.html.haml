              #likeh4b Legacy

              %table{:style=>"margin: 0 0 0 0;text-align:center;"}
                %tr
                  %th{:style=>"width:5%;"}
                  %th{:style=>"width:15%;"}
                  %th{:style=>"width:20%;"}
                  %th{:style=>"width:20%;"}
                  %th{:style=>"width:20%;"}
                  %th{:style=>"width:20%;"}
                - @r5qs.each do |q|
                  -if q[0]=="Header"
                    %tr
                      %td{:colspan=>6}= "&nbsp;".html_safe
                    %tr{:style=>"font-weight:bold;font-size:16px;"}
                      %td{:colspan=>6,:style=>"text-align:left;"}= q[1]
                    %tr
                      %td{:colspan=>6,:style=>"text-align:left;"}= "&nbsp;".html_safe
                    %tr{:style=>"font-weight:bold;"}
                      %td{:colspan=>2}= 'Virginia'
                      %td= 'Uniformed Service Member'
                      %td= 'Overseas Civilians'
                      %td= 'Non UOCAVA'
                      %td= 'Total'
                  -elsif q[0]=~/N\/A/
                    %tr
                      %td{:colspan=>4,:style=>"text-align:left;"}=q[1]+" "+q[2]
                    %tr
                      %td{:colspan=>2}= "&nbsp;".html_safe
                      %td= 'N/A'
                      %td= 'N/A'
                      %td= 'N/A'
                      %td= 'N/A'
                  -elsif q[0]=~/UOD/
                    %tr
                      %td{:colspan=>6,:style=>"text-align:left;"}= q[3]+" "+q[4]
                    %tr
                      %td{:colspan=>2}= "&nbsp;".html_safe
                      -if hash=@vf[q[1]][q[2]]
                        %td= hash['military'].to_s
                        %td= hash['overseas'].to_s
                        %td= hash['domestic'].to_s
                        %td= hash['total'].to_s
                  -else
                    %tr
                      %td{:colspan=>6,:style=>"text-align:left;"}= q[3]+" "+q[4]
                    - %w(A.\ postal B.\ fax C.\ email D.\ online).each do |mode|
                      -if mode!="D\. online" or q[0]=="PFEO"
                        %tr
                          %td= "&nbsp;".html_safe
                          %td{:style=>"text-align:left;"}= mode[0..2]+mode[3..-1].capitalize
                          - mode=mode[3..-1]
                          -if hash=@vf[q[1]][q[2]][mode]
                            %td= hash['military'].to_s
                            %td= hash['overseas'].to_s
                            %td= hash['domestic'].to_s
                            %td= hash['total'].to_s
